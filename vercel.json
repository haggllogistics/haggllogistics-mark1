{
  "framework": "vite",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "outputDirectory": "dist",
  "rewrites": [
    {
      "source": "/haggllogistics-mark1/(.*)",
      "destination": "/$1"
    }
  ]
}